
#include <stdio.h>
#include <math.h> 

/* Change here*/
const N = 200; 




int main(void) {
	
	int next = 0; 


		
	  for(int idx = 1; idx < N; idx++)
	  { 

		if( idx < 2)
		{
		 next = idx +(idx+1); 
		 printf("%d\n", next); 
		}
		else if( idx == 2)
		{
		 next = next + (idx + 1); 
			printf("%d\n", next); 
		}
			else{
						// at this point  idx is 4 anbd res is 6 
			next = next + (idx + 1); 
				printf("%d\n", next); 
			}
			}// end for loop 

	
	  return 0;
}
